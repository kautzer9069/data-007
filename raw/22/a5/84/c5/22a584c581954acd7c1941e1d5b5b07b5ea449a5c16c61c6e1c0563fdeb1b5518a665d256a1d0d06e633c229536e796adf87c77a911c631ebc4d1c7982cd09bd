#include <stdio.h>
#include <iostream>
using std::cout;
using std::endl;

void test()
{
	int * p;
	printf("p = %p\n", p);
}
 
int main(void)
{
	test();
	int * p;
	printf("p = %p\n", p);
	*p = 10;

	return 0;
}
