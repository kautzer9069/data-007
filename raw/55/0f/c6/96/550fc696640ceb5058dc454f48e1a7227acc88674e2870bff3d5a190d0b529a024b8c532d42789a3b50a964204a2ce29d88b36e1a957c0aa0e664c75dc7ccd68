#include "Computer.h"
#include <string.h>
#include <iostream>
using std::cout;
using std::endl;

//inline
void Computer::setBrand(const char * brand)
{	//类内部
	strcpy(_brand, brand);
}


//inline
void Computer::setPrice(float price)
{
	_price = price;
}
 
//inline
void Computer::print()
{
	cout << "brand:" << _brand << endl;
	cout << "price:" << _price << endl;
}

